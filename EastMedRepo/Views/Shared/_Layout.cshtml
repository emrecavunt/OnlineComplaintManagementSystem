<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <title>Eastmed CTMS - @ViewBag.Title </title>

    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <link rel="icon" href="~/assets/images/emulogo.png" type="image/x-icon" />
    <!-- bootstrap & fontawesome -->
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="~/assets/font-awesome/4.5.0/css/font-awesome.min.css" />
    <!-- page specific plugin styles -->
    <link rel="stylesheet" href="@Url.Content("~/assets/css/jquery-ui.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/bootstrap-datepicker3.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/ui.jqgrid.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/Gridmvc.css")" />

    <script href="@Url.Content("~/Scripts/gridmvc.js")"></script>
    @RenderSection("PagePluginStyles", required: false)

    <!-- DataTables Responsive CSS -->
    <link href="@Url.Content("~/assets/datatables-responsive/css/dataTables.responsive.css")" rel="stylesheet" type="text/css" />

    <!-- text fonts -->

    <link rel="stylesheet" href="@Url.Content("~/assets/css/ui.jqgrid.min.css")" />

    <!-- main styles -->

    <link rel="stylesheet" href="@Url.Content("~/assets/css/ace.min.css")" class="ace-main-stylesheet" id="main-ace-style" />

    @Styles.Render("~/bundles/Default")
    <!-- inline styles related to this page -->
    @RenderSection("PageInlineStyles", required: false)


    @Scripts.Render("~/bundles/eastmed")

    <link href="~/Content/sweetalert.css" rel="stylesheet" />

    <link rel="stylesheet" href="@Url.Content("~/Content/typeahead.css")" />

</head>
<body class="no-skin">
    @Html.Partial("_Header")


    <div class="main-container ace-save-state" id="main-container">
        <script type="text/javascript">
            try { ace.settings.loadState('main-container') } catch (e) { }
        </script>
        <script type="text/javascript">
            try { ace.settings.loadState('sidebar') } catch (e) { }
        </script>

        @Html.Partial("_SIDEBAR")
        <div class="main-content">
            <div class="main-content-inner">
                @Html.Partial("_BreadCrumbs")
                <div class="page-content">

                    <div class="row">
                        <div class="col-xs-12 ">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>

        </div>
        @Html.Partial("_Footer")

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>
    </div><!-- /.main-container -->
    <!-- basic scripts -->
    <script src="@Url.Content("~/assets/js/jquery-2.1.4.min.js")"></script>
    <script type="text/javascript">
        window.jQuery || document.write("<script src='/assets/js/jquery.min.js'>" + "<" + "/script>");
    </script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">
        if ('ontouchstart' in document.documentElement) document.write("<script src='~/assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
    </script>
    <script src="@Url.Content("~/assets/js/bootstrap.min.js")"></script>

    <!-- page specific plugin scripts -->
    <script src="@Url.Content("~/assets/js/bootstrap-datepicker.min.js")"></script>
    <script src="@Url.Content("~/assets/js/jquery.jqGrid.min.js")"></script>
    <script src="@Url.Content("~/assets/js/grid.locale-en.js")"></script>

    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/datatables")
    @*@Scripts.Render("~/bundles/vendors")*@
    @RenderSection("PagePluginScripts", required: false)
    @Scripts.Render("~/bundles/elements")
    @Scripts.Render("~/bundles/typeahead")
    @RenderSection("PageInlineScripts", required: false)
    <script src="@Url.Content("~/Scripts/typeahead.mvc.model.js")"></script>

    <script src="@Url.Content("~/assets/js/jquery.jqGrid.min.js")"></script>
    <script src="@Url.Content("~/assets/js/grid.locale-en.js")"></script>
    <script src="~/Scripts/jquery.form.js"></script>
    <script src="~/Scripts/notify.js"></script>
    <script src="~/Scripts/sweetalert.min.js"></script>
    <script src="~/Scripts/bootbox.min.js"></script>
    <script type="text/javascript">

      // Show the message for  when Session Expired
             jQuery(document).ready(function () {



        var SessionValue = '@Request.RequestContext.HttpContext.Session["USERID"]';


        if (SessionValue == "") {
            swal("your session is expired please try to Re-login");
            javascript: document.getElementById('logoutForm').submit();
        }
    });
    </script>

    @*@Scripts.Render("~/bundles/jqwidgets")*@
    <!-- inline scripts related to this page -->
    @RenderSection("scripts", required: false)
</body>
</html>